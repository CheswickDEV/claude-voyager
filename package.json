{
  "name": "claude-voyager",
  "version": "1.0.0",
  "description": "Firefox extension that adds productivity features to claude.ai",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --config vite.config.firefox.ts",
    "build": "tsc --noEmit && vite build --config vite.config.firefox.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@types/dompurify": "^3.2.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/webextension-polyfill": "^0.12.1",
    "@vitejs/plugin-react": "^4.4.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "dompurify": "^3.2.0",
    "jszip": "^3.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "webextension-polyfill": "^0.12.0"
  }
}
